net: "dqn.prototxt"

# test_iter specifies how many forward passes the test should carry out.
# In the case of MNIST, we have test batch size 100 and 100 test iterations,
# covering the full 10,000 testing images.
test_iter: 1

# Carry out testing every x training iterations.
test_interval: 1

# The base lr_policyearning rate, momentum and the weight decay of the network.
base_lr: 0.2
momentum: 0.9
weight_decay: 0.0005

# The learning rate policy
#lr_policy: "fixed"
lr_policy: "step"
gamma: 0.9
stepsize: 500

# Display every x iterations
display: 1

# The maximum number of iterations
max_iter: 200000

# snapshot intermediate results in database.
snapshot: 3000
snapshot_prefix: "caffe_dqn_train"

# solver mode: CPU or GPU
solver_mode: CPU